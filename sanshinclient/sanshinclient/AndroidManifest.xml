<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="jp.gr.java_conf.jyukon.sanshin.sanshinclient" android:versionCode="1" android:versionName="0.0.1-SNAPSHOT">
    <uses-sdk android:minSdkVersion="10"/>

  <application android:icon="@drawable/icon" android:label="@string/app_name">
    <activity android:name=".HelloAndroidActivity" android:screenOrientation="portrait">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="com.google.code.sanshin.sanshinclient.midiplayer.ServerListActivity" android:label="Pick your server" android:screenOrientation="portrait">
	</activity>
	<service android:enabled="true" android:name=".ConnectionService"
	  android:label="@string/app_name" android:icon="@drawable/icon">
	  <intent-filter>
	    <action android:name="com.google.intent.action.BT_ClickLinkCompete_SERVICE" />
	    <category android:name="com.google.intent.category.BT_ClickLinkCompete" />
	  </intent-filter>
	</service>
        
  </application>
  <uses-permission android:name="android.permission.BLUETOOTH" />
  <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
</manifest>

